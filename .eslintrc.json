{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "no-console": "off",
    "indent": "off",
    "object-curly-newline": "off",
    "camelcase": "off",
    "no-underscore-dangle": "off"
  }
}
