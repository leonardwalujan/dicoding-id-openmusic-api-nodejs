{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "no-console": "off",
    "no-underscore-dangle": "off",
    "no-trailing-spaces": "off",
    "indent": "off",
    "linebreak-style": "off",
    "object-curly-newline": "off",
    "camelcase": "off"
  }
}
